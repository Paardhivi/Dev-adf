{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ROCNonProdDF"
		},
		"AzureBlobStorage_connectionString": {
			"type": "string",
			"defaultValue": "DefaultEndpointsProtocol=https;AccountName=rocdevsa;AccountKey=2z8eKCnFTLOwiauqT80Uadn7dYuXXbuuxSbxzFstlU6OdEAzkbIDHP+ihHyFTDKJ0KvHXaenUQrc+AStTDUPYw==;EndpointSuffix=core.windows.net"
		},
		"default_properties_env_value": {
			"type": "string",
			"defaultValue": "dev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/roc-test-',parameters('default_properties_env_value'))]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "[concat('Employee_source_ds_',parameters('default_properties_env_value'))]",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "[concat('Employee_sink_ds_',parameters('default_properties_env_value'))]",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": [],
				"lastPublishTime": "2023-05-15T07:23:02Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Employee_source_ds_',parameters('default_properties_env_value'))]",
				"[concat(variables('factoryId'), '/datasets/Employee_sink_ds_',parameters('default_properties_env_value'))]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Employee_sink_ds_',parameters('default_properties_env_value'))]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "[concat('AzureBlobStorage-',parameters('default_properties_env_value'))]",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "Output",
						"container": "rocthingworx"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"quoteChar": "\""
				},
				"schema": [
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/','AzureBlobStorage-',parameters('default_properties_env_value'))]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Employee_source_ds_',parameters('default_properties_env_value'))]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "[concat('AzureBlobStorage-',parameters('default_properties_env_value'))]",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Employee_source.csv",
						"folderPath": "Input",
						"container": "rocthingworx"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/','AzureBlobStorage-',parameters('default_properties_env_value'))]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/','AzureBlobStorage-',parameters('default_properties_env_value'))]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "DefaultEndpointsProtocol=https;AccountName=rocdevsa;AccountKey=2z8eKCnFTLOwiauqT80Uadn7dYuXXbuuxSbxzFstlU6OdEAzkbIDHP+ihHyFTDKJ0KvHXaenUQrc+AStTDUPYw==;EndpointSuffix=core.windows.net"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/globalparameters",
			"apiVersion": "2018-06-01",
			"properties": {
				"env": {
					"type": "string",
					"value": "[parameters('default_properties_env_value')]"
				}
			},
			"dependsOn": []
		}
	]
}